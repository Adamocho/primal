PRINT "At last..." # A sample comment

# Sample INPUT
INPUT "Gimmie something" user_input

# A variable
LET x = 1
LET is_number = true

# Fizzbuzz
WHILE x <= 15
DO
    IF x % 3 == 0 THEN
        PRINT "Fizz"
        LET is_number = false
    ENDIF

    IF x % 5 == 0 THEN
        PRINT "Buzz"
        LET is_number = false
    ENDIF

    IF is_number == true THEN
        PRINT x
    ENDIF

    LET x = x + 1
    LET is_number = true
ENDWHILE

